pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
            }
        }
        stage('Build') {
            steps {
                echo 'This where we build'
            }
        }
        stage('Test') {
            steps {
                echo 'This is where we test'
            }
        }
        stage('Deploy') {
            steps {
                echo 'And Finally here we Deploy'
            }
        }
    }
}
